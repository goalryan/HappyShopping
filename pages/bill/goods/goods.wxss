@import "../../common/lib/weui.wxss";
.radio-group {
  display: flex;
  flex: 1;
  flex-direction: column;  
  justify-content: center;
  align-items: flex-start;
  
}

.radio-row {
  display: flex;
  flex: 1; 
  width: 100%;
  justify-content: flex-start;
  align-items: center;  
  height: 80rpx;
  padding-left: 32rpx;
  line-height: 80rpx;
  border-bottom: 1rpx solid #d9d9d9;
}

.radio-cell {
  flex: 1;
}
